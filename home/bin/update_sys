#!/bin/bash
echo '----- mariadb -----'
ssh root@db.infinene.net '/usr/bin/apt-get update && /usr/bin/apt-get upgrade -y'
echo
echo '----- mysql -----'
ssh root@mysql.infinene.net '/usr/bin/apt-get update && /usr/bin/apt-get upgrade -y'
echo
echo '----- local -----'
# sudo apt-get update && sudo apt-get upgrade
sudo pacman -Syyu
echo '----- drush-launcher -----'
drush self-update

